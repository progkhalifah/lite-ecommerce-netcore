@model int
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<td style="width: 150px;">
    <div class="btn-group">
        <a asp-action="Edit" asp-controller="ProductType" class="btn btn-primary mr-1" asp-route-id="@Model"><i class="fas fa-edit"></i></a>
        <a asp-action="Details" asp-controller="ProductType" class="btn btn-success mr-1" asp-route-id="@Model"><i class="fas fa-list"></i></a>
        <a asp-action="Delete" asp-controller="ProductType" class="btn btn-danger mr-1" asp-route-id="@Model"><i class="fas fa-trash-alt"></i></a>
    </div>
</td>



